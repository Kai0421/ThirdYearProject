<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/citvirtualmap">


    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rubicon_img_btn"
        android:layout_marginLeft="43dp"
        android:layout_marginStart="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="34dp" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/adminbuilding_img_btn"
        android:layout_marginTop="68dp"
        android:layout_below="@+id/rubicon_img_btn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="61dp"
        android:layout_marginEnd="61dp" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/courtyard_img_bth"
        android:layout_alignTop="@+id/adminbuilding_img_btn"
        android:layout_toRightOf="@+id/rubicon_img_btn"
        android:layout_toEndOf="@+id/rubicon_img_btn"
        android:layout_marginLeft="71dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="21dp" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/student_center_img_btn"
        android:layout_below="@+id/courtyard_img_bth"
        android:layout_alignRight="@+id/courtyard_img_bth"
        android:layout_alignEnd="@+id/courtyard_img_bth"
        android:layout_marginRight="32dp"
        android:layout_marginEnd="32dp" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sports_field_img_btn"
        android:layout_below="@+id/student_center_img_btn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gym_building"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/student_center_img_btn"
        android:layout_toStartOf="@+id/student_center_img_btn" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/library_img_btn"
        android:layout_below="@+id/student_center_img_btn"
        android:layout_toLeftOf="@+id/adminbuilding_img_btn"
        android:layout_toStartOf="@+id/adminbuilding_img_btn"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/it_img_btn"
        android:layout_alignTop="@+id/library_img_btn"
        android:layout_alignLeft="@+id/adminbuilding_img_btn"
        android:layout_alignStart="@+id/adminbuilding_img_btn" />

    <ImageButton
        android:src="@drawable/icon3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/all_weather_sports_field_img_btn"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/sports_field_img_btn"
        android:layout_alignEnd="@+id/sports_field_img_btn"
        android:layout_marginBottom="89dp" />
</RelativeLayout>


